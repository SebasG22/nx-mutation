{
  "version": 1,
  "projects": {
    "demo2": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/demo2",
      "sourceRoot": "apps/demo2/src",
      "prefix": "mutation-demo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/demo2",
            "index": "apps/demo2/src/index.html",
            "main": "apps/demo2/src/main.ts",
            "polyfills": "apps/demo2/src/polyfills.ts",
            "tsConfig": "apps/demo2/tsconfig.app.json",
            "aot": false,
            "assets": ["apps/demo2/src/favicon.ico", "apps/demo2/src/assets"],
            "styles": ["apps/demo2/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo2/src/environments/environment.ts",
                  "with": "apps/demo2/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo2:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/demo2/tsconfig.app.json",
              "apps/demo2/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/demo2/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/demo2/jest.config.js",
            "tsConfig": "apps/demo2/tsconfig.spec.json",
            "setupFile": "apps/demo2/src/test-setup.ts"

          }
        },
        "mutate": {
          "builder": "@pinebox/angular-builder:mutate",
          "options": {
            "mutator": "typescript",
            "packageManager": "yarn",
            "testRunner": "jest",
            "plugins": ["@stryker-mutator/typescript","@stryker-mutator/jest-runner","@stryker-mutator/html-reporter"],
            "jest": {
                "config": "/Users/svelasquezg/Documents/personal/mutation-demo/apps/demo2/jest.config.js"
            },
            "coverageAnalysis": "off",
            "tsconfigFile": "tsconfig.json",
            "files": [ "./libs/**/*.ts",
              "./libs/**/*.html",
              "./apps/demo2/src/**/*.ts",
              "./apps/demo2/src/**/*.html",
              "./apps/demo2/src/**/*.scss",
              "./apps/demo2/**/*.json",
              "./apps/demo2/jest.config.js",
              "./apps/demo2/tsconfig.json",
              "./apps/demo2/package.json"
             ],
            "mutate": [
              "./apps/demo2/src/**/*.ts",
              "./apps/demo2/src/**/*.html",
     "!./apps/demo2/src/**/*.spec.ts"],
            "fileLogLevel": "off",
            "logLevel": "off",
            "timeoutMS": 5000000,
            "reporters": ["html"]
          }
        }
      }
    },
    "demo2-e2e": {
      "root": "apps/demo2-e2e",
      "sourceRoot": "apps/demo2-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo2-e2e/cypress.json",
            "tsConfig": "apps/demo2-e2e/tsconfig.e2e.json",
            "devServerTarget": "demo2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo2:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/demo2-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/demo2-e2e/**"]
          }
        }
      }
    },
    "components": {
      "projectType": "library",
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "prefix": "mutation-demo",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/components/tsconfig.lib.json",
              "libs/components/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/components/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/components/jest.config.js",
            "tsConfig": "libs/components/tsconfig.spec.json",
            "setupFile": "libs/components/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "components-sample": {
      "projectType": "library",
      "root": "libs/components-sample",
      "sourceRoot": "libs/components-sample/src",
      "prefix": "mutation-demo",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/components-sample/tsconfig.lib.json",
              "libs/components-sample/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/components-sample/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/components-sample/jest.config.js",
            "tsConfig": "libs/components-sample/tsconfig.spec.json",
            "setupFile": "libs/components-sample/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "demo"
}
